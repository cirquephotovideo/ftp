{
  "name": "ftp-utility",
  "version": "1.0.0",
  "description": "FTP file retrieval utility with history tracking",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^1.5.0",
    "basic-ftp": "^5.0.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-schedule": "^2.1.1",
    "sqlite3": "^5.1.6",
    "ssh2-sftp-client": "^9.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
